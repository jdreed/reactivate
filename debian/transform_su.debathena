#!/bin/sed -f
# /ClusterLogin exists and is empty within a cluster login session.
# If it exists, skip the pam_echo and die rule.
1iauth [ignore=1 success=ok] pam_echo.so file=/ClusterLogin
1iauth [default=die] pam_echo.so file=/etc/athena/su-error
/^@include common-auth$/iauth optional pam_echo.so file=/etc/athena/su-warning
